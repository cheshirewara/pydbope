{
    "python.pythonPath": "venv\\Scripts\\python.exe",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--indent-size=4",
        "--ignore=E402"
    ],
    "[python]": {
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "editor.formatOnPaste": false,
        "editor.formatOnType": false,
      },
}